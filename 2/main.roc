parseRow = \rowStr ->
    row = Str.split rowStr " "
    oppStr = row 
        |> List.first
        |> Result.withDefault "error"

    ownStr = row 
        |> List.last
        |> Result.withDefault "error"  
    
    opp = when oppStr is
        "A" -> Rock
        "B" -> Paper
        "C" -> Scissors
        _ -> crash "unknown opponent throw \(oppStr)"
    
    own = when ownStr is 
        "X" -> X
        "Y" -> Y
        "Z" -> Z
        _ -> crash "unknown onw throw \(ownStr)"

    EncryptedGame opp own

parsedInput = input
    |> Str.split "\n"
    |> List.map parseRow

part1 = scoreInput parsedInput translateStrategy |> Num.toStr
part2 = scoreInput parsedInput matchStrategy |> Num.toStr

main =
    Stdout.line "part 1: \(part1) part 2: \(part2)"
